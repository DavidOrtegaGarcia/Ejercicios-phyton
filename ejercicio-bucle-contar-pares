# Inicializaciones
salir = "N"
limite= input ("Escriba el limite?:")
num=0

while ( salir=="N" ):
    # Hago cosas
    print num

    # Incremento
    num=num+2
    # Activo indicador de salida si toca
    if ( numero>limite ): # CondiciÃ³n de salida
            salir = "S"
